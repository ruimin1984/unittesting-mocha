{
    "extension": [
        ".ts",
        ".tsx",
        ".js"
    ],
    "exclude": [
        "**/*.d.ts",
        "**/test/*"
    ],
    "include": [
        "src/*",
        "build/src/*"
    ],
    "reporter": [
        "html",
        "text",
        "text-summary"
    ],
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "report-dir": "./test_report",
    "all": true
}